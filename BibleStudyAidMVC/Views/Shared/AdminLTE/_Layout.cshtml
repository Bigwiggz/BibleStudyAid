<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Bible Study Aid | Main</title>
    <!--Styles Partial-->
    <partial name="AdminLTE/_Styles" />

    @*Bootstrap CSS CDN*@
   <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    @RenderSection("Styles", required: false)

    @*Syncfusion Controls*@
    <link rel="stylesheet" href="https://cdn.syncfusion.com/ej2/20.1.47/material.css" />
    <script src="https://cdn.syncfusion.com/ej2/20.1.47/dist/ej2.min.js"></script>

</head>
<body class="hold-transition sidebar-mini">
<div class="wrapper">

  <!--Top Navigation Partial-->
  <partial name="AdminLTE/_TopNavigation" />

  <!--Main Navigation Partial-->
